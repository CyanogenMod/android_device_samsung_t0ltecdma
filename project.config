[project]
[receive]
	requireContributorAgreement = true
[access "refs/*"]
	owner = group Administrators
[access "refs/heads/*"]
	create = group sbrissen
	push = group sbrissen
	label-Code-Review = -2..+2 group sbrissen
	label-Verified = -1..+1 group sbrissen
	submit = group sbrissen
[access "refs/tags/*"]
	exclusiveGroupPermissions = create
	create = group sbrissen
	push = group sbrissen
[submit]
	action = rebase if necessary
